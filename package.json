{
  "name": "@klekh/ask",
  "version": "0.1.5",
  "description": "AI driven UI",
  "main": "index.ask",
  "type": "module",
  "scripts": {
    "test": "exit 1",
    "release": "npm version patch && npm publish --access=public",
    "deploy": "gh-pages -d public",
    "server": "node ./server/server.js",
    "server_watch": "nodemon ./server/server.js",
    "cli": "node ./engine/cli.js",
    "task": "node ./engine/task.js",
    "transpile": "node ./engine/transpile.js",
    "docs": "npm run task ./public/docs.ask ./public/docs.html && open ./public/docs.html",
    "web": "npm run task ./public/index.ask ./public/index.html && open ./public/index.html"
  },
  "keywords": [
    "ask",
    "ai",
    "rfc",
    "dataflow",
    "language",
    "platform",
    "concept",
    "protocol",
    "pattern",
    "framework"
  ],
  "author": "Konstantin Lekh",
  "dependencies": {
    "chalk": "^5.4.1",
    "dotenv": "^16.4.7",
    "gh-pages": "^6.3.0",
    "openai": "^4.86.0",
    "stable-hash": "^0.0.4"
  }
}
